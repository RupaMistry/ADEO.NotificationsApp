<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Language" content="en">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"> 
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no"> 
    <title> @ViewBag.PageTitle - ADEO.NotificationsApp</title>
    <!-- Disable tap highlight on IE -->
    <meta name="msapplication-tap-highlight" content="no">
    <!-- <link href="./vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet"> -->
    <link rel="stylesheet" href="~/lib/architectui/vendors/fortawesome/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/lib/architectui/vendors/ionicons-npm/css/ionicons.css">
    <link rel="stylesheet" href="~/lib/architectui/vendors/linearicons-master/dist/web-font/style.css">
    <link rel="stylesheet" href="~/lib/architectui/vendors/pixeden-stroke-7-icon-master/pe-icon-7-stroke/dist/pe-icon-7-stroke.css">
    <link href="~/css/base.css" rel="stylesheet">
    <link rel="stylesheet" href="~/lib/ckeditor5/style.css">
    <link rel="stylesheet" href="https://cdn.ckeditor.com/ckeditor5/43.0.0/ckeditor5.css">
</head>
<body>
    <header></header>
    <div class="app-container app-theme-white body-tabs-shadow fixed-header fixed-sidebar">
        <div class="app-main">
            <div class="app-sidebar sidebar-shadow">
                <div class="app-header__logo">
                    <div class="logo-src"></div>
                    <div class="header__pane ms-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic" data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button" class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>
                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu">
                            <li class="app-sidebar__heading">Menu</li>
                            <li class="mm-active">
                                <a href="#">
                                    <i class="metismenu-icon pe-7s-rocket"></i>
                                    People Notifications Dashboard
                                    <i class="metismenu-state-icon pe-7s-angle-down caret-left"></i>
                                </a>
                                <ul>
                                    <li>
                                        <a style="pointer-events:none" href="/meeting" class="mm-active">
                                            <i class="metismenu-icon"></i>
                                            All Notifications
                                        </a>
                                    </li>
                                    <li>
                                        <a style="pointer-events:none" href="/attendeesAction">
                                            <i class="metismenu-icon"></i>
                                            All History
                                        </a>
                                    </li> 
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="app-main__outer">
                <div class="app-main__inner">
                    <div class="app-page-title">
                        <div class="page-title-wrapper">
                            <div class="page-title-heading">
                                <div class="page-title-icon">
                                    <i class="lnr-calendar-full icon-gradient bg-arielle-smile icon-gradient fa-meetup bg-mean-fruit"></i>
                                </div>
                                <div>
                                    @ViewBag.PageTitle
                                   </div>
                            </div>
                            <div class="page-title-actions">
                              @*   <button type="button" data-bs-toggle="tooltip" title="New Notification"
                                        data-bs-placement="bottom" class="btn-shadow me-3 btn btn-dark">
                                    <i class="fa fa-plus"></i>
                                </button> *@
                                <button id="btnMessageEditor" type="button" class="btn-shadow me-3 btn btn-dark"
                                        data-bs-toggle="modal" data-bs-target=".bd-example-modal-lg">
                                    <i class="fa fa-plus"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div>
                        @RenderBody()
                    </div> 
                </div>
                <div class="app-wrapper-footer">
                    <div class="app-footer">
                        <div class="app-footer__inner">
                            <div class="app-footer-left">
                                <div class="footer-dots">
                                </div>
                            </div> 
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
     
    <div class="app-drawer-overlay d-none animated fadeIn"></div>
    <!-- plugin dependencies -->
    <script type="text/javascript" src="~/lib/architectui/vendors/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/moment/moment.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/metismenu/dist/metisMenu.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/bootstrap4-toggle/js/bootstrap4-toggle.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/jquery-circle-progress/dist/circle-progress.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/toastr/build/toastr.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/jquery.fancytree/dist/jquery.fancytree-all-deps.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/apexcharts/dist/apexcharts.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/bootstrap-table/dist/bootstrap-table.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/slick-carousel/slick/slick.min.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/chenfengyuan/datepicker/dist/datepicker.js"></script>
    <script type="text/javascript" src="~/lib/architectui/vendors/daterangepicker/daterangepicker.js"></script>
    <!-- custome.js -->
    <script type="text/javascript" src="~/js/charts/apex-charts.js"></script>
    <script type="text/javascript" src="~/js/circle-progress.js"></script>
    <script type="text/javascript" src="~/js/demo.js"></script>
    <script type="text/javascript" src="~/js/scrollbar.js"></script>
    <script type="text/javascript" src="~/js/toastr.js"></script>
    <script type="text/javascript" src="~/js/treeview.js"></script>
    <script type="text/javascript" src="~/js/form-components/toggle-switch.js"></script>
    <script type="text/javascript" src="~/js/tables.js"></script>
    <script type="text/javascript" src="~/js/carousel-slider.js"></script>
    <script type="text/javascript" src="~/js/app.js"></script>  
    <script type="text/javascript" src="~/js/notification-app/userMessage.js"></script>
    <script type="text/javascript" src="~/js/form-components/datepicker.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

@{
    await Html.RenderPartialAsync("_MessageEditorPartial");
}